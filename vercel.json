{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
     { "src": "/search.html", "dest": "public/search.html" },
     { "src": "/(.*\\.html|.*\\.css|.*\\.js|.*\\.png|.*\\.svg|.*\\.json|.*\\.mp3|.*\\.wav)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "functions": {
    "server.js": { "maxDuration": 60 }
  }
}
